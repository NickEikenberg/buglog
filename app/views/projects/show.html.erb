<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>


<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Branch</th>
      <th>Severity</th>
      <th>Priority</th>
      <th>Reported By</th>
      <th>Assigned To</th>
      <th>Status</th>
      <th>Date Created</th>
    </tr>
  </thead>
  
  <tbody>
    <% @bugs.each do |bug| %>
      <tr>
        <th><%= bug.id %></th>
        <td><%= link_to bug.title, bug  %></td>
        <td><%= bug.branch %></td>
        <td><%= bug.severity %></td>
        <td><%= bug.priority %></td>
        <td><%= bug.reported_by %></td>
        <td><%= bug.assigned_to %></td>
        <td><%= bug.status %></td>
        <td><%= bug.created_at %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
